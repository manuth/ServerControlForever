{
    "name": "manuth/server-control-forever",
    "description": "An easy-to-use automated TrackMania Forever Server Controller for increasing the User Experience on your TMF server.",
    "keywords": [
        "console",
        "cli",
        "TrackMania Forever",
        "tmnf",
        "tmuf"
    ],
    "homepage": "https://github.com/manuth/ServerControlForever",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/manuth/ServerControlForever/issues",
        "source": "https://github.com/manuth/ServerControlForever"
    },
    "authors": [
        {
            "name": "Manuel Thalmann",
            "email": "m@nuth.ch"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/database": "^9.0",
        "illuminate/log": "^9.0",
        "laravel-zero/framework": "^9.0",
        "nunomaduro/termwind": "^1.3",
        "symfony/filesystem": "^6.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.4",
        "pestphp/pest": "^1.21.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "scf"
    ]
}
